package Tugas_Miinggu_4;

/**
 *
 * @author ACER
 */
public class Tugas_2_Perkalian2 {
    public static void main(String[] args) {
      System.out.println("          Do_While          ");
      System.out.println("Bilangan Perkalian 2 (0-100)");
      System.out.println("============================");
        for(int i=2; i<100; i*=2) {
            System.out.print(i + " ");
        }
    
    }
    
}
